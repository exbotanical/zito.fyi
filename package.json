{
  "name": "zito.fyi",
  "version": "2.0.0",
  "description": "Matthew Zito's personal blog site",
  "keywords": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exbotanical/zito.fyi.git"
  },
  "author": "Matthew Zito <exbotanical@protonmail.com>",
  "scripts": {
    "dev": "gatsby develop -p 3000 -o",
    "build": "npm run clean && gatsby build",
    "serve": "gatsby serve",
    "check": "npm run lint:fix && npm run lint:css:fix && npm run lint:md:fix && npm run typecheck && npm run fmt",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "fmt": "prettier --write \"**/*.{ts,tsx,md,css,scss}\"",
    "lint": "eslint --ext .ts,.js,.tsx,.jsx,.json .",
    "lint:css": "stylelint \"src/**/*.{css,scss,ts,tsx}\"",
    "lint:md": "markdownlint \"**/*.{md,mdx}\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx,.jsx,.json . --fix",
    "lint:css:fix": "stylelint \"src/**/*.{css,scss,ts,tsx}\" --fix",
    "lint:md:fix": "markdownlint \"**/*.{md,mdx}\" --fix",
    "purge": "rimraf .cache && rimraf public && rimraf node_modules",
    "test:cy:dev": "CYPRESS_SUPPORT=y CYPRESS_BASE_URL=http://localhost:3000 CYPRESS_STAGE=dev start-server-and-test dev http://localhost:3000 cy:open",
    "test:cy": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:open",
    "test:cy:ci": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:run",
    "test:unit": "jest",
    "test:unit:dev": "jest --watchAll",
    "test": "npm run test:unit && npm run test:cy:ci",
    "test:snapshot": "jest -u",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "npm run typecheck -- --watch"
  },
  "bugs": {
    "url": "https://github.com/exbotanical/zito.fyi/issues"
  },
  "homepage": "https://github.com/exbotanical/zito.fyi#readme",
  "engines": {
    "node": ">=18"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,css,scss,json,yml,yaml}": [
      "npm run fmt"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@fec/remark-a11y-emoji": "3.1.0",
    "@mdx-js/react": "^2.3.0",
    "@styled-icons/boxicons-logos": "^10.47.0",
    "@styled-icons/boxicons-regular": "^10.47.0",
    "@styled-icons/styled-icon": "^10.7.1",
    "date-fns": "2.28.0",
    "gatsby": "^5.14.1",
    "gatsby-plugin-catch-links": "^5.14.0",
    "gatsby-plugin-disqus": "^1.2.6",
    "gatsby-plugin-feed": "^5.14.0",
    "gatsby-plugin-google-gtag": "^5.14.0",
    "gatsby-plugin-image": "^3.14.0",
    "gatsby-plugin-manifest": "^5.14.0",
    "gatsby-plugin-mdx": "^5.14.0",
    "gatsby-plugin-offline": "^6.14.0",
    "gatsby-plugin-react-helmet": "^6.14.0",
    "gatsby-plugin-react-helmet-canonical-urls": "^1.4.0",
    "gatsby-plugin-sharp": "^5.14.0",
    "gatsby-plugin-sitemap": "^6.14.0",
    "gatsby-plugin-styled-components": "^6.14.0",
    "gatsby-plugin-svgr-svgo": "^2.0.0",
    "gatsby-plugin-twitter": "^5.14.0",
    "gatsby-remark-copy-linked-files": "^6.14.0",
    "gatsby-remark-embed-video": "^3.2.1",
    "gatsby-remark-images": "^7.14.0",
    "gatsby-remark-prismjs": "^7.14.0",
    "gatsby-remark-relative-images-v2": "^0.1.5",
    "gatsby-remark-responsive-iframe": "^6.14.0",
    "gatsby-source-filesystem": "^5.14.0",
    "gatsby-transformer-remark": "^6.14.0",
    "gatsby-transformer-sharp": "^5.14.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.isobject": "^3.0.2",
    "lodash.kebabcase": "^4.1.1",
    "prism-themes": "1.9.0",
    "prismjs": "^1.27.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "6.1.0",
    "react-query": "^3.39.3",
    "react-share": "^5.1.2",
    "reading-time": "^1.5.0",
    "remark-comment": "^1.0.0",
    "remark-external-links": "8.0.0",
    "remark-gfm": "^1.0.0",
    "remark-unwrap-images": "2.1.0",
    "remove-markdown": "0.3.0",
    "source-map-support": "0.5.21",
    "styled-components": "^6.1.15",
