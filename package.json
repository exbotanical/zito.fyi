{
	"name": "zito.dev",
	"version": "2.0.0",
	"description": "Matthew Zito's personal blog site",
	"keywords": [],
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MatthewZito/zito.dev.git"
	},
	"author": "Matthew Zito <matthewtzito@gmail.com> (goldmund)",
	"scripts": {
		"dev": "gatsby develop -p 3000 -o",
		"build": "gatsby build",
		"serve": "gatsby serve",
		"check": "pnpm lint && pnpm lint:css && pnpm lint:md && pnpm typecheck && pnpm format",
		"clean": "gatsby clean",
		"cy:open": "cypress open",
		"cy:run": "cypress run",
		"format": "prettier --write \"src/**/*.{ts,tsx,md,css,scss}\"",
		"lint": "eslint --ext .ts,.js,.tsx,.jsx,.json . --fix",
		"lint:css": "stylelint \"src/**/*.{css,scss,ts,tsx}\" --fix",
		"lint:md": "markdownlint \"**/*.{md,mdx}\" --fix",
		"test:cy:dev": "CYPRESS_SUPPORT=y CYPRESS_BASE_URL=http://localhost:3000 CYPRESS_STAGE=dev start-server-and-test dev http://localhost:3000 cy:open",
		"test:cy": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:open",
		"test:cy:ci": "CYPRESS_SUPPORT=y npm run build && start-server-and-test serve http://localhost:9000 cy:run",
		"test:unit": "jest",
		"test:unit:dev": "jest --watchAll",
		"test": "pnpm test:unit && pnpm test:cy:ci",
		"typecheck": "tsc --noEmit",
		"typecheck:watch": "pnpm typecheck -- --watch"
	},
	"bugs": {
		"url": "https://github.com/MatthewZito/zito.dev/issues"
	},
	"homepage": "https://github.com/MatthewZito/zito.dev#readme",
	"lint-staged": {
		"**/*.{ts,tsx,js,jsx,css,scss,json,yml,yaml}": [
			"pnpm format"
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
